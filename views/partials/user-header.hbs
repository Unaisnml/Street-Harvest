<!-- Header Section Begin -->
<header class="header">
  <div class="container-fluid">
    <div class="row">
      <div class="col-xl-3 col-lg-2 sticky">
        <div class="header__logo">
          <a href="/">
            <img
              src="/product-images/logo.png"
              style="height:2rem ;"
              alt="logo"
            />
          </a>
          {{! <a href="./index.html"><img src="user/assets/img/logo.png" alt=""></a> }}
        </div>
      </div>
      <div class="col-xl-6 col-lg-7">
        <nav class="header__menu ">
          <ul>
            <li class=""><a href="/">Home</a></li>
            {{#each category}}
              <li class="" data-filter="*"><a
                  href="/category?category={{this.category}}"
                >{{this.category}}</a></li>
            {{/each}}

            <li><a href="/contact">Contact</a></li>
          </ul>
        </nav>
      </div>
      <div class="col-lg-3">
        <div class="header__right">
          <div class="header__right__auth">
            {{#if users}}
          

              <div class="btn-group">
<button type="button" class="btn btn-danger dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
    {{users.Name}}
  </button>
  <ul class="dropdown-menu">
    <li><a class="dropdown-item font-weight-bold text-primary" href="/profile">My Profile</a></li>
    <li><a class="dropdown-item font-weight-bold text-primary" href="/orders">My Orders</a></li>
    <li><a class="dropdown-item font-weight-bold text-primary" href="/userLogout">Logout</a></li>
  </ul>
</div>
            {{else}}
              <div class="btn-group">
  <button type="button" class="btn btn-danger dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
    Account
  </button>
  <ul class="dropdown-menu">
    <li><a class="dropdown-item font-weight-bold text-primary" href="/userLogin">Login</a></li>
    <li><a class="dropdown-item font-weight-bold text-primary" href="/userSignup">Signup</a></li>
  </ul>
</div>
            {{/if}}
          </div>
          <ul class="header__right__widget">
            {{!-- <li><span class="icon_search search-switch"></span></li> --}}
            {{#if users}}
              <li><a href="/wishList"><span class="icon_heart_alt"></span>
                  <div class="tip" id="whishlist-count">{{wishListCount}}</div>
                </a></li>
              <li><a href="/cart"><span class="icon_bag_alt"></span>
                  <div class="tip" id="cart-count">{{cartCount}}</div>
                </a></li>
            {{/if}}
          </ul>
        </div>
      </div>
    </div>
    <div class="canvas__open">
      <i class="fa fa-bars"></i>
    </div>
  </div>
</header>
<!-- Header Section End -->
{{{body}}}

